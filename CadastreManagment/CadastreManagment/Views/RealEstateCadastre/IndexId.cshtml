@model IEnumerable<CadastreManagment.RealEstateCadastre>

@{
    ViewBag.Title = "IndexId";
}

<h2 class="zagolovok">Ваши кадастры недвижимости</h2>

<table class="table">
    <tr>
        <th>
            Тип недвижимости:
        </th>
        <th>
            Площадь:
        </th>
        <th>
            Адрес:
        </th>
        <th>
            Дата регистрации:
        </th>
        <th>
            Паспортные данные владельца:
        </th>
        <th>
            Готовность объекта:
        </th>
        <th>
            Назаначение объекта:
        </th>
        <th>
            Этажность:
        </th>
        <th>
            Налоговый коэффициент:
        </th>
        <th>
            Заметки:
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Address)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Approval_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Passport)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Readiness)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Appointment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Floors)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tax_coefficint)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Notes)
            </td>
            <td>
                @Html.ActionLink("Налоги", "IndexId", "Tax", new { cadastreId = item.Id, cadastreType = "RealEstateCadastre" }, new { @class = "edit" })
            </td>
        </tr>
    }

</table>

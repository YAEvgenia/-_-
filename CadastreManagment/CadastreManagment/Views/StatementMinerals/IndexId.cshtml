@model IEnumerable<CadastreManagment.StatementMinerals>

@{
    ViewBag.Title = "IndexId";
}

<h2 class="zagolovok">Ваши заявления на кадастр природных ископаемых</h2>

<p>
    @Html.ActionLink("Создать новое", "Create", null, new { @class = "edit" })
</p>
<table class="table">
    <tr>
        <th>
            Статус заявления
        </th>
        <th>
            Тип природных ископаемых
        </th>
        <th>
            Адрес
        </th>
        <th>
            Стоимость
        </th>
        <th>
            Паспортные данные владельца
        </th>
        <th>
            Объем
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Passport)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Amount)
        </td>
        
    </tr>
}

</table>

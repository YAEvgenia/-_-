@model CadastreManagment.LandRegistry

@{
    ViewBag.Title = "Create";
}

<h2 class="zagolovok">Создание земельного кадастра</h2>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div >
        <p class="parametr" style="color:black">Земельный кадастр</p>
        <hr />
        @Html.ValidationSummary(true, "")
        <div >
            Площадь:
            <div >
                @Html.EditorFor(model => model.Area)
                @Html.ValidationMessageFor(model => model.Area, "")
            </div>
        </div>

        <div >
            Адрес:
            <div >
                @Html.EditorFor(model => model.Address)
                @Html.ValidationMessageFor(model => model.Address, "")
            </div>
        </div>

        <div >
            Стоимость:
            <div >
                @Html.EditorFor(model => model.Price)
                @Html.ValidationMessageFor(model => model.Price, "")
            </div>
        </div>

        <div >
            Дата внесения в кадастр:
            <div >
                @Html.EditorFor(model => model.ApprovalDate)
                @Html.ValidationMessageFor(model => model.ApprovalDate, "")
            </div>
        </div>

        <div >
            Паспортные данные владельца:
            <div >
                @Html.EditorFor(model => model.Passport)
                @Html.ValidationMessageFor(model => model.Passport, "")
            </div>
        </div>

        <div >
            Налоговый коэффициент:
            <div >
                @Html.EditorFor(model => model.Tax_coefficint)
                @Html.ValidationMessageFor(model => model.Tax_coefficint, "")
            </div>
        </div>
        <br />

        <div>
            <div>
                <input type="submit" value="Создать" />|
                @Html.ActionLink("Вернуться к списку", "Index", null, new { @class = "details" })

            </div>
        </div>
    </div>
}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}

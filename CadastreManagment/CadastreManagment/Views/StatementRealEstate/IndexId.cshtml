@model IEnumerable<CadastreManagment.StatementRealEstate>

@{
    ViewBag.Title = "IndexId";
}

<h2 class="zagolovok">Ваши заявления на кадастр недвижимости</h2>

<p>
    @Html.ActionLink("Создать новый", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Статус
        </th>
        <th>
            Тип недвижимости
        </th>
        <th>
            Площадь
        </th>
        <th>
            Адрес
        </th>
        <th>
            Паспортные данные владельца
        </th>
        <th>
            Готовность объекта
        </th>
        <th>
            Назначение объекта
        </th>
        <th>
            Этажность
        </th>

    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Area)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Passport)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Readiness)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Appointment)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Floors)
        </td>
       
    </tr>
}

</table>
